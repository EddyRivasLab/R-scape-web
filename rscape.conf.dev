# rename this file to rscape.yml and put a ':' after 'name' if
# you want to use YAML like in old versions of Catalyst
name R-scape

<View HTML>
  WRAPPER "layouts/wrapper.tt"
</View>

<Model Rscape>
  dir_path    /tmp/rscape
  rscape_dir  /build/rscape_v2.0.0.i
  gnuplot     /usr/local/bin/gnuplot
  gnuplot_ps  /usr/local/share/gnuplot/gnuplot/5.0/PostScript
</Model>
